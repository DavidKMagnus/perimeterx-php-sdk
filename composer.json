{
  "name": "perimeterx/php-sdk",
  "description": "PerimeterX SDK for PHP",
  "keywords": [
    "perimeterx",
    "websecurity",
    "security",
    "bots",
    "sdk"
  ],
  "type": "library",
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "php": "^5.6|^7.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-0": {
      "Perimeterx\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.4.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Facebook\\Tests\\": "tests/"
    }
  }
}